<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" 
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
    <display-name>Hospital</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>to Create and Search patient data</description>
    <display-name>PatientManagement</display-name>
    <servlet-name>PatientManagement</servlet-name>
    <servlet-class>com.testhospital.PatientManagement</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PatientManagement</servlet-name>
    <url-pattern>/PatientManagement</url-pattern>
  </servlet-mapping>  
  
</web-app>